<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/base_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:onClick="jumpToActivity"
        android:text="BaseQuicAdapterActivity基本用法"
        tools:background="@color/orange"></Button>

    <Button
        android:id="@+id/multi_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:onClick="jumpToActivity"
        android:text="BaseMultiItemQuickAdapterActivity基本用法"></Button>

</LinearLayout>